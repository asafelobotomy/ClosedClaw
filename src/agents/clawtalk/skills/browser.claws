# ENGINE
<script lang="typescript">
import { browser, screenshot, screenshotRegion, screenshotOcr, webSearch, currentTime } from "@closedclaw/tools";

export async function execute(args: BrowserArgs) {
  switch (args.action) {
    case "navigate":
      return browser.navigate(args.url);
    case "screenshot":
      return screenshot(args.region);
    case "fill":
      return browser.fill(args.selector, args.value);
    case "click":
      return browser.click(args.selector);
    case "extract":
      return browser.extract(args.extractQuery);
    default:
      return browser.navigate(args.url);
  }
}
