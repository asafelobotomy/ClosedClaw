{
  "name": "ClosedClaw",
  "version": "2026.2.1",
  "description": "AI assistant gateway that connects multiple messaging channels with the Pi agent runtime",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "bin": {
    "ClosedClaw": "ClosedClaw.mjs"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "ClosedClaw.mjs",
    "README-header.png",
    "README.md",
    "assets/**",
    "dist/**",
    "docs/**",
    "extensions/**",
    "git-hooks/**",
    "patches/**",
    "tools/ci/format-staged.js",
    "tools/dev/postinstall.js",
    "tools/ci/setup-git-hooks.js",
    "tools"
  ],
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./plugin-sdk": "./dist/plugin-sdk/index.js",
    "./plugin-sdk/*": "./dist/plugin-sdk/*",
    "./cli-entry": "./ClosedClaw.mjs"
  },
  "scripts": {
    "ClosedClaw": "node tools/dev/run-node.mjs",
    "ClosedClaw:rpc": "node tools/dev/run-node.mjs agent --mode rpc --json",
    "android:assemble": "cd apps/android && ./gradlew :app:assembleDebug",
    "android:install": "cd apps/android && ./gradlew :app:installDebug",
    "android:run": "cd apps/android && ./gradlew :app:installDebug && adb shell am start -n ai.ClosedClaw.android/.MainActivity",
    "android:test": "cd apps/android && ./gradlew :app:testDebugUnitTest",
    "build": "pnpm canvas:a2ui:bundle && tsc -p tsconfig.json --noEmit false && node --import tsx tools/build/canvas-a2ui-copy.ts && node --import tsx tools/build/copy-hook-metadata.ts && node --import tsx tools/build/write-build-info.ts",
    "canvas:a2ui:bundle": "bash tools/build/bundle-a2ui.sh",
    "check": "pnpm tsgo && pnpm lint && pnpm format",
    "check:loc": "node --import tsx tools/dev/check-ts-max-loc.ts --max 500",
    "deps:audit": "pnpm audit --audit-level moderate",
    "deps:outdated": "pnpm outdated",
    "deps:update": "pnpm update --latest --interactive",
    "dev": "node tools/dev/run-node.mjs",
    "dev:agent": "node tools/dev/run-node.mjs agent",
    "dev:gateway": "node tools/dev/run-node.mjs gateway --verbose",
    "dev:tui": "node tools/dev/run-node.mjs tui",
    "docs:bin": "node tools/docs/build-docs-list.mjs",
    "docs:build": "cd docs && pnpm dlx --reporter append-only mint broken-links",
    "docs:dev": "cd docs && mint dev",
    "docs:list": "node tools/docs/docs-list.js",
    "doctor": "node tools/dev/run-node.mjs doctor",
    "format": "oxfmt --check",
    "format:check": "oxfmt --check",
    "format:fix": "oxfmt --write",
    "gateway:dev": "ClosedClaw_SKIP_CHANNELS=1 node tools/dev/run-node.mjs --dev gateway",
    "gateway:dev:reset": "ClosedClaw_SKIP_CHANNELS=1 node tools/dev/run-node.mjs --dev gateway --reset",
    "gateway:watch": "node tools/dev/watch-node.mjs gateway --force",
    "lint": "oxlint --type-aware --tsconfig tsconfig.oxlint.json",
    "lint:check": "oxlint --type-aware --tsconfig tsconfig.oxlint.json",
    "lint:fix": "oxlint --type-aware --tsconfig tsconfig.oxlint.json --fix && pnpm format:fix",
    "plugins:sync": "node --import tsx tools/maintenance/sync-plugin-versions.ts",
    "postinstall": "node tools/dev/postinstall.js",
    "prepack": "pnpm build && pnpm ui:build",
    "protocol:gen": "node --import tsx tools/maintenance/protocol-gen.ts",
    "release:check": "node --import tsx tools/maintenance/release-check.ts",
    "start": "node tools/dev/run-node.mjs",
    "status": "node tools/dev/run-node.mjs status",
    "test": "node tools/testing/test-parallel.mjs",
    "test:all": "pnpm lint && pnpm build && pnpm test && pnpm test:ui && pnpm test:e2e && pnpm test:live && pnpm test:docker:all",
    "test:changed": "vitest run --changed",
    "test:coverage": "vitest run --coverage",
    "test:docker:all": "pnpm test:docker:live-models && pnpm test:docker:live-gateway && pnpm test:docker:onboard && pnpm test:docker:gateway-network && pnpm test:docker:qr && pnpm test:docker:doctor-switch && pnpm test:docker:plugins && pnpm test:docker:cleanup",
    "test:docker:cleanup": "bash tools/testing/test-cleanup-docker.sh",
    "test:docker:doctor-switch": "bash tools/testing/e2e/doctor-install-switch-docker.sh",
    "test:docker:gateway-network": "bash tools/testing/e2e/gateway-network-docker.sh",
    "test:docker:live-gateway": "bash tools/testing/test-live-gateway-models-docker.sh",
    "test:docker:live-models": "bash tools/testing/test-live-models-docker.sh",
    "test:docker:onboard": "bash tools/testing/e2e/onboard-docker.sh",
    "test:docker:plugins": "bash tools/testing/e2e/plugins-docker.sh",
    "test:docker:qr": "bash tools/testing/e2e/qr-import-docker.sh",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:extensions": "vitest run --config vitest.extensions.config.ts",
    "test:force": "node --import tsx tools/testing/test-force.ts",
    "test:gateway": "vitest run --config vitest.gateway.config.ts",
    "test:install:e2e": "bash tools/testing/test-install-sh-e2e-docker.sh",
    "test:install:e2e:anthropic": "ClosedClaw_E2E_MODELS=anthropic bash tools/testing/test-install-sh-e2e-docker.sh",
    "test:install:e2e:openai": "ClosedClaw_E2E_MODELS=openai bash tools/testing/test-install-sh-e2e-docker.sh",
    "test:install:smoke": "bash tools/testing/test-install-sh-docker.sh",
    "test:live": "ClosedClaw_LIVE_TEST=1 vitest run --config vitest.live.config.ts",
    "test:security": "vitest run src/security/ src/commands/audit-query.test.ts src/commands/keychain.test.ts src/commands/keys-management.test.ts src/commands/skill-sign.test.ts src/agents/skill-verification.test.ts",
    "test:ui": "pnpm --dir ui test",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:watch": "vitest",
    "test:watch:unit": "vitest --config vitest.unit.config.ts",
    "tui": "node tools/dev/run-node.mjs tui",
    "tui:dev": "ClosedClaw_PROFILE=dev node tools/dev/run-node.mjs --dev tui",
    "ui:build": "node tools/dev/ui.js build",
    "ui:dev": "node tools/dev/ui.js dev",
    "ui:install": "node tools/dev/ui.js install"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "0.13.1",
    "@aws-sdk/client-bedrock": "^3.980.0",
    "@clack/prompts": "^1.0.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "@homebridge/ciao": "^1.3.4",
    "@line/bot-sdk": "^10.6.0",
    "@lydell/node-pty": "1.2.0-beta.3",
    "@mariozechner/pi-agent-core": "0.51.0",
    "@mariozechner/pi-ai": "0.51.0",
    "@mariozechner/pi-coding-agent": "0.51.0",
    "@mariozechner/pi-tui": "0.51.0",
    "@mozilla/readability": "^0.6.0",
    "@noble/ciphers": "^1.3.0",
    "@noble/hashes": "^1.7.0",
    "@sinclair/typebox": "0.34.48",
    "@slack/bolt": "^4.6.0",
    "@slack/web-api": "^7.13.0",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "ajv": "^8.17.1",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.3",
    "croner": "^10.0.1",
    "discord-api-types": "^0.38.38",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "file-type": "^21.3.0",
    "grammy": "^1.39.3",
    "jiti": "^2.6.1",
    "json5": "^2.2.3",
    "jszip": "^3.10.1",
    "linkedom": "^0.18.12",
    "long": "^5.3.2",
    "markdown-it": "^14.1.0",
    "node-edge-tts": "^1.2.9",
    "osc-progress": "^0.3.0",
    "pdfjs-dist": "^5.4.624",
    "playwright-core": "1.58.1",
    "proper-lockfile": "^4.1.2",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.5",
    "signal-utils": "^0.21.1",
    "sqlite-vec": "0.1.7-alpha.2",
    "tar": "7.5.7",
    "tslog": "^4.10.2",
    "undici": "^7.20.0",
    "ws": "^8.19.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@grammyjs/types": "^3.23.0",
    "@img/sharp-linux-x64": "^0.34.5",
    "@lit-labs/signals": "^0.2.0",
    "@lit/context": "^1.1.6",
    "@types/express": "^5.0.6",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.2.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript/native-preview": "7.0.0-dev.20260201.1",
    "@vitest/coverage-v8": "^4.0.18",
    "lit": "^3.3.2",
    "ollama": "^0.6.3",
    "oxfmt": "0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.4",
    "rolldown": "1.0.0-rc.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@napi-rs/canvas": "^0.1.89",
    "node-llama-cpp": "^3.15.0"
  },
  "peerDependenciesMeta": {
    "@napi-rs/canvas": {
      "optional": true
    },
    "node-llama-cpp": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "packageManager": "pnpm@10.23.0",
  "pnpm": {
    "minimumReleaseAge": 2880,
    "overrideComments": {
      "tough-cookie": "CVE-2023-26136 fix. Pulled transitively via archived @matrix-org/matrix-sdk-crypto-nodejs â†’ request@2.88.2. Re-evaluate when Matrix SDK dependency is fully removed. @removeBy 2026.6.0",
      "form-data": "Pinned to 2.5.5 (same transitive chain as tough-cookie). @removeBy 2026.6.0"
    },
    "overrides": {
      "fast-xml-parser": "5.3.4",
      "form-data": "2.5.5",
      "@hono/node-server>hono": "4.11.7",
      "hono": "4.11.7",
      "qs": "6.14.1",
      "@sinclair/typebox": "0.34.48",
      "tar": "7.5.7",
      "tough-cookie": "4.1.3"
    },
    "onlyBuiltDependencies": [
      "@lydell/node-pty",
      "@napi-rs/canvas",
      "@whiskeysockets/baileys",
      "esbuild",
      "node-llama-cpp",
      "protobufjs",
      "sharp"
    ]
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "reporter": [
        "text",
        "lcov"
      ],
      "thresholds": {
        "lines": 70,
        "functions": 70,
        "branches": 70,
        "statements": 70
      },
      "include": [
        "src/**/*.ts"
      ],
      "exclude": [
        "src/**/*.test.ts"
      ]
    },
    "include": [
      "src/**/*.test.ts"
    ],
    "exclude": [
      "dist/**",
      "**/vendor/**"
    ]
  }
}
